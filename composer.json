{
    "name": "offdev/gpp",
    "type": "library",
    "description": "A wrapper around guzzle that provides middleware functionality for responses, URL enumeration capabilities, and a crawler to make use of it",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Pascal Severin",
            "email": "pascal@offdev.net"
        }
    ],
    "require": {
        "php": ">=7.1",
        "guzzlehttp/guzzle": "^6.3",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "infection/infection": "^0.10.5",
        "phpunit/phpunit": "^7.3",
        "squizlabs/php_codesniffer": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "Offdev\\Gpp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Offdev\\Tests\\": "tests/"
        }
    }
}